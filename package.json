{
  "name": "treasuredata",
  "displayName": "treasuredata",
  "description": "Create, edit, and run queries directly from VS Code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.40.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:tdQueries",
    "onCommand:extension.authenticate",
    "onCommand:exentsion.runQuery",
    "onCommand:exentsion.refresh"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.authenticate",
        "title": "Treasure Data: Edit your API Key"
      },
      {
        "command": "extension.runQuery",
        "title": "Treasure Data: Run the current query",
        "when": "viewItem == tdQuery"
      },
      {
        "command": "extension.newQuery",
        "title": "New query"
      },
      {
        "command": "extension.refresh",
        "title": "Treasure Data: Refresh list of queries"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "tdQueries",
          "name": "TD Queries"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "extension.newQuery",
          "when": "view == tdQueries"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.11.7",
    "@types/node-fetch": "^2.5.3",
    "@types/vscode": "^1.40.0",
    "glob": "^7.1.5",
    "mocha": "^6.2.2",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4",
    "vscode-test": "^1.2.2"
  },
  "dependencies": {
    "node-fetch": "^2.6.1"
  }
}
